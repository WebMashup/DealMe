<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:header="urn:import:me.deal.client.view.header"
	xmlns:menubar="urn:import:me.deal.client.view.menubar"
	xmlns:main="urn:import:me.deal.client.view.main">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel>
		<header:HeaderWidget ui:field="headerWidget"></header:HeaderWidget>
		<menubar:MenuWidget ui:field="menuWidget"></menubar:MenuWidget>
		<g:Hidden>
			<menubar:LocationWidget ui:field="locationWidget"></menubar:LocationWidget>
			<menubar:FilterWidget ui:field="filterWidget"></menubar:FilterWidget>
		</g:Hidden>
		<g:HorizontalPanel>
			<main:ListWidget ui:field="listWidget"></main:ListWidget>
			<main:GoogleMapWidget ui:field="googleMapWidget"></main:GoogleMapWidget>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 